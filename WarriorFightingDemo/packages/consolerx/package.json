{
	"name": "consolerx",
	"version": "1.3.0",
	"description": "Custom Your Console Output Styles",
	"author": "RazgrizHsu <fly@raz.tw>",
	"main": "index.js",
	"main-menu":
	{
		"i18n:MAIN_MENU.panel.title/ConsoleRx":
		{
			"icon": "icon.png",
			"accelerator": "CmdOrCtrl+9",
			"message": "consolerx:open"
		}
	},
	"panel":
	{
		"main": "panel/index.js",
		"popable": true,
		"type": "dockable",
		"title": "ConsoleRx",
		"icon": "icon.png",
		"width": 600,
		"height": 350,
		"min-width": 465,
		"min-height": 300,
		"shortcuts":
		{
			"command+k": "clear",
			"ctrl+k": "clear"
		},
		"profiles":
		{
			"project":
			{
				"fontsize":			11,
				"lineheight":		26,
				"fontfamilies": 	"Monaco",

				"colors":
				{
					"log":			"#999999",
					"error":		"#DA2121",
					"warn":			"#999900",
					"info":			"#0099FF",
					"failed":		"#DA2121",
					"success":		"#009900"
				},
				"IgnorePatterns": 	"",
				"AutoClean":		false
			}
		}
	},
	"panel-settings":
	{
		"main": "panel-settings/index.js",
		"type": "fixed-size",
		"popable": true,
		"title": "ConsoleRx-Settings",
		"icon": "icon.png",
		"width": 500,
		"height": 350
	},
	"profiles":
	{
		"project":
		{
			"fontsize":			11,
			"lineheight":		26,
			"fontfamilies": 	"Monaco",

			"colors":
			{
				"log":			"#999999",
				"error":		"#DA2121",
				"warn":			"#999900",
				"info":			"#0099FF",
				"failed":		"#DA2121",
				"success":		"#009900"
			},
			"IgnorePatterns": 	"",
			"AutoClean":		false
		}
	}
}
